drop table if exists user;
create table user(
	id int auto_increment,
	name varchar(100),
	age varchar(3),
	PRIMARY KEY (id)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

desc user;
select * from user;

insert into user ( name,age) values ('张三','22');

--记事本
drop table if exists jsb;
create table jsb(
	id int auto_increment,
	title varchar(50),
	text varchar(1000),
	remark varchar(1000),
	update_time timestamp default current_timestamp(),
	PRIMARY KEY (id)
)ENGINE=InnoDB DEFAULT CHARSET=utf8;

--文本、日记



